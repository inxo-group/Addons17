<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <data noupdate="1">
        <record id="view_country_state_tree_inherited_opengeek" model="ir.ui.view">
            <field name="name">res.country.state.tree.inherited.opengeek</field>
            <field name="model">res.country.state</field>
            <field name="inherit_id" ref="base.view_country_state_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='code']" position="after">
                    <field name="code_do"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
